{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser":true,
        "node": true
    },
    "rules": {
        "object-curly-spacing": "off",
        "require-jsdoc": "off",
        "operator-linebreak": ["error", "none", { "overrides": {"=": "after", "?": "before", ":": "before"}}],
        "indent": "off"
    },
    "extends": ["eslint:recommended", "google"],
    "plugins": [
        "prettier"
    ]

}