{
    "parser": "babel-eslint",
    "env": {
        "jest/globals": true,
        "es6": true,
        "browser":true,
        "node": true
    },
    "rules": {
        "object-curly-spacing": "off",
        "require-jsdoc": "off",
        "max-len": "off",
        "operator-linebreak": ["error", "none", { "overrides": {".": "before", "=": "after", "?": "before", ":": "before"}}],
        "indent": "off",
        "newline-per-chained-call": ["error", { "ignoreChainWithDepth": 2 }],
        "no-invalid-this": "off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}